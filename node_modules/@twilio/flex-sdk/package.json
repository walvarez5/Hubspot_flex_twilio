{
  "name": "@twilio/flex-sdk",
  "version": "0.99.32-dev",
  "description": "Twilio Headless Flex Client for Web Applications",
  "main": "./index.js",
  "module": "./esm/index.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./taskrouter": {
      "require": {
        "types": "./taskrouter/index.d.ts",
        "default": "./taskrouter/index.js"
      },
      "import": {
        "types": "./taskrouter/index.d.ts",
        "default": "./esm/taskrouter/index.js"
      }
    },
    "./cbm": {
      "require": {
        "types": "./cbm/index.d.ts",
        "default": "./cbm/index.js"
      },
      "import": {
        "types": "./cbm/index.d.ts",
        "default": "./esm/cbm/index.js"
      }
    },
    "./data-client": {
      "require": {
        "types": "./data-client/index.d.ts",
        "default": "./data-client/index.js"
      },
      "import": {
        "types": "./data-client/index.d.ts",
        "default": "./esm/data-client/index.js"
      }
    },
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      }
    }
  },
  "keywords": [
    "flex",
    "sdk"
  ],
  "author": "Twilio",
  "license": "Apache-2.0",
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@segment/analytics-node": "1.3.0",
    "@twilio/voice-sdk": "^2.11.0",
    "buffer": "^6.0.3",
    "core-js": "^3.19.3",
    "crypto-js": "^4.2.0",
    "events": "^3.3.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "5.15.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.0",
    "query-string": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "twilio-taskrouter": "^2.0.12",
    "twilsock": "^0.8.3",
    "ws": "^8.18.0"
  },
  "peerDependencies": {
    "graphql": "^16.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "graphql": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  }
}